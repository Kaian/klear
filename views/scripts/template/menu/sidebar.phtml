{{each(sec,section) sections}}
<h2>${section.sectionName}</h2>
<div>
<ul>
{{each(idx,subsection) section.subsections}}
<li>
<a class="subsection{{if (section.subsections[idx].default)}} autoplay{{/if}}" href="?" id="target-${subsection.sectionId}">
<span class="ui-silk inline ${subsection.sectionClass}"></span>
${subsection.sectionName}
</a>
</li>
{{/each}}
</ul>
</div>
{{/each}}
